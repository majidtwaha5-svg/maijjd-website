{
  "version": 2,
  "buildCommand": "cd frontend_maijjd && npm install && npm run build",
  "outputDirectory": "frontend_maijjd/build",
  "installCommand": "cd frontend_maijjd && npm install",
  "framework": "create-react-app",
  "routes": [
    {
      "source": "/api/(.*)",
      "destination": "https://maijjd-backend-production-ad65.up.railway.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://maijjd-backend-production-ad65.up.railway.app/api"
  },
  "build": {
    "env": {
      "CI": "false"
    }
  }
}
