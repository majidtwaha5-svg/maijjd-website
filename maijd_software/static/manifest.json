  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "prefer_related_applications": false,
  "related_applications": [],
  "protocol_handlers": [
    {
      "protocol": "web+maijd",
      "url": "/mobile-dashboard.html?action=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/mobile-dashboard.html?action=open&file=%s",
      "accept": {
        "application/json": [".json"],
        "text/plain": [".txt", ".log"],
        "application/xml": [".xml"]
      }
    }
  ],
  "share_target": {
    "action": "/mobile-dashboard.html?action=share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["*/*"]
        }
      ]
    }
  },
  "permissions": [
    "notifications",
    "background-sync",
    "storage",
    "geolocation"
  ],
  "background": {
    "service_worker": "/sw.js",
    "type": "module"
  },
  "display_override": [
    "standalone",
    "minimal-ui",
    "window-controls-overlay"
  ],
  "window_controls_overlay": {
    "tag": "title-bar",
    "visibility": "auto"
  },
  "capture_links": "existing-client-navigate",
  "iarc_rating_id": "e84b072d-71e3-4d3e-8ae9-bd6c5f11d9ea",
